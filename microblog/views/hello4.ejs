    <!DOCTYPE html>
	<html lang="zh-CN">
   <script src="http://libs.baidu.com/jquery/1.10.2/jquery.min.js">
    </script>
    <script  src="javascripts/1.js">
    </script>
           <h1>User List</h1>
    
    
    <table class="colorful">
        <thead>
            <tr>
                <th>id</th>
                <th>userid</th>
                <th>password</th>

            </tr>
        </thead>
        <tbody>
            
            <% user.forEach(function(user ){ %>
            <tr>
                <td><%=user._id%></td>
                <td><%= user.userid%></td>
                <td><%= user.password %></td>
            <% }); %>
        </tbody>
    </table>
    <button type="button" id="btn">toggle</button>
    </html>